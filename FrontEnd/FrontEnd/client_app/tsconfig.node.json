{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.vite/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "vite.config.ts"
  ]
}